extends layout 

block content
    table(class = 'gridtable')
        tr
            th Name
            th Age
            th Email
            th Last Visited
            th
            tr
                td #{users.username}
                td #{users.age}
                td #{users.email} 
                td #{cookie}
                td 
                    a(href='/edit/' + users._id) Edit info
    p(id='lastVisit') 
        a(href='/admin/' + users._id) #{access}

        br
        br
        a(href="/logout")
            button(type="button") Log Out
    